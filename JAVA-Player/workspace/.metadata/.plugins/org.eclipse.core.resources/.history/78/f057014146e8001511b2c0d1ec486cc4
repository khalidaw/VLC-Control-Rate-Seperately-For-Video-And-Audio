import java.awt.image.BufferedImage;
import java.io.File;
import java.io.IOException;

import javax.imageio.ImageIO;

public class getIcons {
	static int width = 16;
	static int height = 16;
	
	public BufferedImage getPlayButton()
	{
		BufferedImage buttonIcon;
		try {
			buttonIcon = ImageIO.read(new File("_PNG64/music_bell.png"));
		} catch (IOException e) {
			// TODO Auto-generated catch block
			return null;
		}
		return (BufferedImage) buttonIcon.getScaledInstance(width, height, 0);
	}
}
