import java.awt.EventQueue;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JTextField;


public class popUpData {

	private JFrame frame;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					popUpData window = new popUpData();
					window.frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the application.
	 */
	public popUpData() {
		initialize();
	}

	/**
	 * Initialize the contents of the frame.
	 */
	private void initialize() {
		int height = 10;
		int width = 300;
		
		frame = new JFrame();
		frame.setBounds(100, 100, width, 300);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		int countHeight = 0;
		
		// audio text
		JLabel audioLabel = new JLabel("Audio speed:");
		JTextField audioText = new JTextField("1.0");
		audioLabel.setBounds(0, 0, width, 10);
		countHeight+=10;
		audioLabel.setBounds(0, 0, width, countHeight);

		
		
	}

}
