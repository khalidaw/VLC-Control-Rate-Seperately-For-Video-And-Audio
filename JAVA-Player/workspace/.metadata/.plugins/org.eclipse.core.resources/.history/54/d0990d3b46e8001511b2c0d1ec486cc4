import java.awt.image.BufferedImage;
import java.io.File;

import javax.imageio.ImageIO;

public class getIcons {
	static int width = 16;
	static int height = 16;
	
	public BufferedImage getPlayButton()
	{
		BufferedImage buttonIcon = ImageIO.read(new File("_PNG64/music_bell.png"));
		return buttonIcon.getScaledInstance(width, height, 0);
	}
}
